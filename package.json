{
  "name": "MovieApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:android": "react-native run-android --mode=debug",
    "start:ios": "react-native run-ios",
    "lint": "eslint .",
    "clean:yarn": "rm -rf node_modules/ && yarn cache clean && yarn install --silent --non-interactive",
    "clean:watchman": "watchman watch-del-all",
    "clean:android": "(cd android && ./gradlew --stop && rm -rf ~/.gradle && ./gradlew clean && rm -rf app/build/)",
    "clean:ios:build-folder": "rm -rf ios/build",
    "clean:ios:pods-folder": "rm -rf ios/Pods",
    "clean:ios:user-pods-cache": "rm -rf ~/.cocoapods",
    "clean:ios:pods-lock": "rm -rf ios/Podfile.lock",
    "clean:ios:pods-cache": "cd ios && pod cache clean --all && cd ../",
    "clean:ios:reinstall-pods": "cd ios && pod install && cd ../",
    "clean:ios": "yarn clean:ios:build-folder && yarn clean:ios:pods-folder && yarn clean:ios:user-pods-cache && yarn clean:ios:pods-lock && yarn clean:ios:pods-cache && yarn clean:ios:reinstall-pods",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/native": "7.1.19",
    "@react-navigation/native-stack": "7.6.2",
    "@reduxjs/toolkit": "2.10.1",
    "i18n-js": "4.5.1",
    "native-base": "3.4.28",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-config": "1.6.0",
    "react-native-gesture-handler": "2.29.1",
    "react-native-localize": "3.6.0",
    "react-native-reanimated": "4.1.5",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.18.0",
    "react-native-svg": "15.14.0",
    "react-native-svg-transformer": "1.5.2",
    "react-native-worklets": "0.6.1",
    "react-redux": "9.2.0",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@babel/runtime": "7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "29.5.13",
    "@types/react": "19.1.1",
    "@types/react-test-renderer": "19.1.0",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "9.39.1",
    "jest": "29.6.3",
    "metro-react-native-babel-preset": "0.77.0",
    "patch-package": "8.0.1",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
